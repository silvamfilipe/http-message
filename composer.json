{
    "name": "fsilva/http-message",
    "description": "PSR Http Message compliant package",
    "keywords": [
        "fsilva",
        "http-message",
        "PSR",
        "PSR Http Message"
    ],
    "homepage": "https://github.com/silvamfilipe/http-message",
    "license": "MIT",
    "authors": [
        {
            "name": "Filipe Silva",
            "email": "silvam.filipe@gmail.com",
            "homepage": "https://github.com/silvamfilipe",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.4.0",
        "psr/http-message": "1.0.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "Fsilva\\HttpMessage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fsilva\\HttpMessage\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev",
            "dev-develop": "0.3-dev"
        }
    }
}
